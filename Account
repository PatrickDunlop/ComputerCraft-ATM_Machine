term.clear()
term.setCursorPos(1,1) --Standard setup
print("Account Info") --Temporary print

local function back()

 shell.run("disk/atm/atm") --Standard code to return to main screen and stop running this code
 
end

while true do

 local event, key = os.pullEvent("key")
 if key == 14 then
 
  back()
  
 end
 
end
